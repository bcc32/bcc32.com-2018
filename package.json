{
  "name": "visitor-log",
  "version": "0.1.3",
  "description": "Records visitors to the site and allows them to post messages",
  "main": "index.js",
  "scripts": {
    "build:bsb": "bsb -make-world",
    "build:js": "rollup -c",
    "build:lessc": "lessc client/style.less dist/style.css",
    "build:code": "run-s build:bsb build:js",
    "build:styles": "run-s build:lessc",
    "build": "run-p build:code build:styles",
    "watch:bsb": "bsb -w",
    "watch:js": "rollup -c -w",
    "prewatch": "run-s build",
    "watch": "run-p watch:bsb watch:js",
    "lint": "eslint --fix .",
    "dev": "forever start config/development.json",
    "start": "forever start config/production.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Aaron L. Zeng <a2z@mit.edu> (https://github.com/bcc32/)",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "commander": "^2.11.0",
    "compression": "^1.7.0",
    "express": "^4.15.3",
    "express-winston": "^2.4.0",
    "highlight.js": "^9.12.0",
    "mkdirp": "^0.5.1",
    "pug": "^2.0.0-rc.2",
    "sqlite3": "^3.1.8",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "bootstrap": "^3.3.7",
    "bs-platform": "^1.8.1",
    "bucklescript-tea": "^0.6.2",
    "eslint": "^3.19.0",
    "jquery": "^3.2.1",
    "less": "^2.7.2",
    "moment": "^2.18.1",
    "npm-run-all": "^4.0.2",
    "rollup": "^0.45.2",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^4.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcc32/visitor-log.git"
  },
  "bugs": {
    "url": "https://github.com/bcc32/visitor-log/issues"
  },
  "homepage": "https://github.com/bcc32/visitor-log#readme"
}
